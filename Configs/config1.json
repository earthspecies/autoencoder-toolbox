{
	"dataset_config":{
		"n_train_samples": 4000,
		"n_test_samples": 1000,
		"signal_params":{
			"min_freq": 50,
			"min_delta": 1000,
			"max_freq": 8000,
			"max_delta": 5000,
			"duration": 1,
			"sample_rate": 16000,
			"ramp": true
		}
	},
	"model_config":{
		"encoder_config":{
			"model_name": "HeavyConv",
			"model_params": {
				"in_size": [null, 1, 16000],
				"nfft": 400,
				"hop": 160,
				"hidden_units": 128,
				"n_res_blocks": 1,
				"n_down": 1,
				"latent_dim": 64,
				"clamp": true,
				"vae": false,
				"verbose": false
			}
		},
		"decoder_config":{
			"model_name": "HeavyConv",
			"model_params":{
				"in_size": [null, 64, 51],
				"out_size": [null, 1, 16000],
				"nfft": 400,
				"hop": 160,
				"hidden_units": 128,
				"n_res_blocks": 1,
				"n_up": 1,
				"latent_dim": 64,
				"clamp": true,
				"verbose": false,
				"crop": 1
			}
		},
		"model_params":{
			"vae":false,
			"vq_vae":false,
			"irmae":false
		}
	},
	"learning_params":{
		"batch_size": 32,
		"learning_rate": 3e-4,
		"epochs": 50
	},
	"trainer_params":{
		"verbose": 1,
		"device": "cuda",
		"dest": "Config1Test",
		"params":{
			"L2_regularizer_callback": {
				"lambda": 1e-5
			},
			"model_saver_callback": {
				"epoch": 1,
				"save_every":1
			}
		}
	}
}
